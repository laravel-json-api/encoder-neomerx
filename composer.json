{
    "name": "laravel-json-api/encoder-neomerx",
    "description": "Encode JSON API resources using the neomerx/json-api package.",
    "keywords": [
        "jsonapi.org",
        "json-api",
        "jsonapi",
        "json:api",
        "laravel"
    ],
    "homepage": "https://github.com/laravel-json-api/{NAME}",
    "support": {
        "issues": "https://github.com/laravel-json-api/{NAME}/issues"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Cloud Creativity Ltd",
            "email": "info@cloudcreativity.co.uk"
        },
        {
            "name": "Christopher Gammie",
            "email": "contact@gammie.co.uk"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../core/",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "laravel-json-api/core": "*",
        "neomerx/json-api": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelJsonApi\\Encoder\\Neomerx\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelJsonApi\\Encoder\\Neomerx\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev",
            "dev-develop": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
